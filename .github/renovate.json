{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:recommended"
    ],
    "prHourlyLimit": 0,
    "prConcurrentLimit": 0,
    "packageRules": [
        {
            "matchPackageNames": [
                "org.spockframework:spock-core"
            ],
            "enabled": false
        }
    ],
    "automerge": true,
    "labels": [
        "type: dependencies"
    ],
    "commitMessagePrefix": "chore(deps):"
}
